<h1>Listagem dos usuarios</h1>

<table>
	<tr>
		<th>Nome</th>
		<th>Email</th>
	<tr>
	<% @users.each do user %>
	<tr>
		<td><% => user.name <%></td>
		<td><% => user.email <%></td>
		<td><%= link_to "Show", user <%></td>
		
	</tr>
	<% end %>
</table>

<%= link_to "Novo tema", new.user_path %>
		
		
	