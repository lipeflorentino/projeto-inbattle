<% provide(:title, 'Todos Administradores') %>
<h1>Todos os Administradores</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>

      <%= link_to user.name, user %>
	    <% if current_user.admin? && !current_user?(user) %>
      <%= link_to "Apagar Admin", user, method: "delete" %>
  <% end %>
    </li>
  <% end %>
</ul>


