<% provide(:title, 'Todos os candidatos') %>
<h1>Todos os Candidatos</h1>

<ul class="candidatos">
  <% @candidato.each do |cand| %>
    <li><%= cand.endereco %>
	<%= link_to image_tag(cand.endereco, alt: "Candidato 1", width: 100, high: 100), '#' %>
	</li>
	<li>Pontos: <%= cand.pontos %>

  <% end %>
</ul>

